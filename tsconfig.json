{
  "include": ["lib/**/*"],
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react",
    "importHelpers": false,
    "esModuleInterop": true,
    "outDir": "dist",
    "declaration": true,
    "module": "commonjs",
    "rootDir": "lib",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "target": "ES5"
  },
}